<%- include('header'); %>
<%- include('sidebar'); %>

      <div id="content-wrapper">

        <div class="container-fluid">



          <div class="row">


            
            <div class="col-12 col-md-4 mb-3 chart">
              <div class="chartTitle"><p> Males / Females Users </p></div>
              <canvas data-males="<%= chartsData.males %>" data-females="<%= chartsData.females %>" id="male.females" width="100" height="100"></canvas>
            </div>

            <div class="col-12 col-md-4 mb-3 chart">
              <div class="chartTitle"><p> Unread / Read Messages </p></div>
              <canvas data-unread="<%= chartsData.noReadMsg %>" data-read="<%= chartsData.readMsg %>" id="read.unread" width="100" height="100"></canvas>
            </div>

            <div class="col-12 col-md-4 mb-3 chart">
              <div class="chartTitle"><p> Unshipped / shipped Orders </p></div>
              <canvas data-unshiped="<%= chartsData.unshiped %>" data-shiped="<%= chartsData.shiped %>" id="shiped.unshiped" width="100" height="100"></canvas>
            </div>

            <div class="col-12 col-md-4 mb-3 chart">
              <div class="chartTitle"><p> Sales Line </p></div>
              <canvas data-sales="<%=JSON.stringify(payedOrders)%>"   id="saleLine" width="100" height="100"></canvas>
            </div>
            
          </div>

        </div>
        <!-- /.container-fluid -->

<%- include('footer'); %>
